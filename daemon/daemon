#!/usr/bin/env python3
import socket
import sys

def main():
    # Access to the web server
    hostname, port = sys.argv[1:]

    # Server socket
    srvsock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
    srvsock.bind(('0.0.0.0', 12345))
    srvsock.listen(1)


    while True:
        (s, addr) = srvsock.accept()


if __name__ == '__main__':
    main()
